#include<stdio.h>
void main(){
    int i=0,j=0,a=0 ,b=0,c=0;

    j= i++ + ++i;
    printf("%d i1",i);
    printf("\n");

    a= i-- + --i;
    printf("%d i2",i);
    printf("\n");

    b= i++ - ++i;
    printf("%d i3",i);
    printf("\n");

    c= i-- - --i;
    printf("%d i4",i);
    printf("\n");

    printf("%d j",j);
    printf("\n");

    printf("%d a",a);
    printf("\n");

    printf("%d b",b);
    printf("\n");

    printf("%d c",c);
}
